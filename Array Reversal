#include <stdio.h>
int main(){
    int n, i, sum=0;
    printf("Enter number of the elements:\n");
    scanf("%d", &n);
    int arr[n];

    for (i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }
    printf("Reverse array elements:\n");
    for (i =n - 1; i >= 0; i--) {
            printf("%d\n", arr[i]);
    }
    return 0;
}

